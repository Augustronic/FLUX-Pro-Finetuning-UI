# FLUX-Pro-Finetuning-UI - Environment Variables Example
# Copy this file to .env and customize the values for your environment

# API Configuration
FLUX_API_KEY=your_api_key_here
FLUX_API_ENDPOINT=https://api.us1.bfl.ai
FLUX_API_TIMEOUT=30
FLUX_API_MAX_RETRIES=3

# Environment
FLUX_ENV=development  # Options: development, production, test

# Logging
FLUX_LOG_LEVEL=INFO  # Options: DEBUG, INFO, WARNING, ERROR, CRITICAL
FLUX_LOG_FILE=logs/app.log
FLUX_LOG_FORMAT=json  # Options: json, text

# Performance
FLUX_CACHE_SIZE=1024
FLUX_REQUEST_TIMEOUT=30
FLUX_MAX_CONCURRENT_JOBS=5

# Security
FLUX_RATE_LIMIT=60
FLUX_ALLOWED_ORIGINS=localhost,127.0.0.1
FLUX_TOKEN_EXPIRY=3600

# Feature Flags
FLUX_FEATURE_IMAGE_PROMPT_SUPPORT=false
FLUX_FEATURE_PROMPT_UPSAMPLING=true
FLUX_FEATURE_ADVANCED_PARAMETERS=true

# Storage
FLUX_STORAGE_PATH=data
FLUX_MAX_FILE_SIZE=10485760  # 10MB in bytes